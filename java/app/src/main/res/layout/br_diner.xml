<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@drawable/bg_rectangle_black73_47_cr10"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_margin="50dp">
    <RelativeLayout
        android:background="@drawable/bg_rectangle_orange_hgr_crt10_1"
        android:layout_width="match_parent"
        android:layout_height="30dp">
        <TextView
            android:textColor="@color/white"
            android:gravity="center"
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/dinner_title_name_dialog"
            android:fontFamily="@font/muller_bold"
            android:layout_marginStart="10dp"/>
        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/close_button"
            android:background="@drawable/ic_close_single"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginTop="5dp"
            android:layout_centerVertical="true"
            android:layout_marginEnd="25dp"
            android:layout_alignParentEnd="true"/>
    </RelativeLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="5dp"
        android:weightSum="3">
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/br_diner_hotdog"
            android:background="@drawable/bg_button_gray"
            android:layout_width="0px"
            android:layout_height="match_parent"
            android:layout_marginLeft="2.5dp"
            android:layout_marginRight="2.5dp"
            android:weightSum="2"
            android:layout_weight="1">
            <FrameLayout
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/img_red_ellipse"/>
                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="15dp"
                    app:srcCompat="@drawable/img_hotdog"/>
            </FrameLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <TextView
                    android:textSize="9sp"
                    android:textColor="@color/orange"
                    android:id="@+id/textView9"
                    android:background="#00ffffff"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/dinner_product_hotdog"
                    android:fontFamily="@font/muller_bold"/>
                <TextView
                    android:textSize="8sp"
                    android:textColor="@color/white"
                    android:id="@+id/br_diner_price_0"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/muller_bold"/>
                <TextView
                    android:textSize="8sp"
                    android:textColor="@color/white"
                    android:id="@+id/br_diner_satiety_0"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/muller_bold"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/br_diner_cheburek"
            android:background="@drawable/bg_button_gray"
            android:layout_width="0px"
            android:layout_height="match_parent"
            android:layout_marginLeft="2.5dp"
            android:layout_marginRight="2.5dp"
            android:weightSum="2"
            android:layout_weight="1">
            <FrameLayout
                android:background="@drawable/img_red_ellipse"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    app:srcCompat="@drawable/img_cheburek"/>
            </FrameLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <TextView
                    android:textSize="9sp"
                    android:textColor="@color/orange"
                    android:id="@+id/textView13"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/dinner_product_cheburek"
                    android:fontFamily="@font/muller_bold"/>
                <TextView
                    android:textSize="8sp"
                    android:textColor="@color/white"
                    android:id="@+id/br_diner_price_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/muller_bold"/>
                <TextView
                    android:textSize="8sp"
                    android:textColor="@color/white"
                    android:id="@+id/br_diner_satiety_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/muller_bold"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/br_diner_pie"
            android:background="@drawable/bg_button_gray"
            android:layout_width="0px"
            android:layout_height="match_parent"
            android:layout_marginLeft="2.5dp"
            android:layout_marginRight="2.5dp"
            android:weightSum="2"
            android:layout_weight="1">
            <FrameLayout
                android:background="@drawable/img_red_ellipse"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginStart="10dp"
                    app:srcCompat="@drawable/img_pie"/>
            </FrameLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <TextView
                    android:textSize="9sp"
                    android:textColor="@color/orange"
                    android:id="@+id/textView19"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/dinner_product_pie"
                    android:fontFamily="@font/muller_bold"/>
                <TextView
                    android:textSize="8sp"
                    android:textColor="@color/white"
                    android:id="@+id/br_diner_price_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/montserrat_bold"/>
                <TextView
                    android:textSize="8sp"
                    android:textColor="@color/white"
                    android:id="@+id/br_diner_satiety_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/muller_bold"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp">
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/br_diner_sausage"
            android:background="@drawable/bg_button_gray"
            android:layout_width="0px"
            android:layout_height="match_parent"
            android:layout_marginLeft="2.5dp"
            android:layout_marginRight="2.5dp"
            android:weightSum="2"
            android:layout_weight="1">
            <FrameLayout
                android:background="@drawable/img_red_ellipse"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    app:srcCompat="@drawable/img_sausage"/>
            </FrameLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <TextView
                    android:textSize="9sp"
                    android:textColor="@color/orange"
                    android:id="@+id/textView22"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/dinner_product_sausage"
                    android:fontFamily="@font/montserrat_bold"/>
                <TextView
                    android:textSize="8sp"
                    android:textColor="@color/white"
                    android:id="@+id/br_diner_price_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/montserrat_bold"/>
                <TextView
                    android:textSize="8sp"
                    android:textColor="@color/white"
                    android:id="@+id/br_diner_satiety_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/muller_bold"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/br_diner_coffee"
            android:background="@drawable/bg_button_gray"
            android:layout_width="0px"
            android:layout_height="match_parent"
            android:layout_marginLeft="2.5dp"
            android:layout_marginRight="2.5dp"
            android:weightSum="2"
            android:layout_weight="1">
            <FrameLayout
                android:background="@drawable/img_red_ellipse"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    app:srcCompat="@drawable/img_coffe"/>
            </FrameLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <TextView
                    android:textSize="9sp"
                    android:textColor="@color/orange"
                    android:id="@+id/textView25"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/dinner_product_coffee"
                    android:fontFamily="@font/muller_bold"/>
                <TextView
                    android:textSize="8sp"
                    android:textColor="@color/white"
                    android:id="@+id/br_diner_price_4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/muller_bold"/>
                <TextView
                    android:textSize="8sp"
                    android:textColor="@color/white"
                    android:id="@+id/br_diner_satiety_4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/muller_bold"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/br_diner_tea"
            android:background="@drawable/bg_button_gray"
            android:layout_width="0px"
            android:layout_height="match_parent"
            android:layout_marginLeft="2.5dp"
            android:layout_marginRight="2.5dp"
            android:weightSum="2"
            android:layout_weight="1">
            <FrameLayout
                android:background="@drawable/img_red_ellipse"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/img_tea"/>
            </FrameLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <TextView
                    android:textSize="9sp"
                    android:textColor="@color/orange"
                    android:id="@+id/textView28"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/dinner_product_tea"
                    android:fontFamily="@font/muller_bold"/>
                <TextView
                    android:textSize="8sp"
                    android:textColor="@color/white"
                    android:id="@+id/br_diner_price_5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/muller_bold"/>
                <TextView
                    android:textSize="8sp"
                    android:textColor="@color/white"
                    android:id="@+id/br_diner_satiety_5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/muller_bold"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="34dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="14dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="16dp"
        android:weightSum="5">
        <FrameLayout
            android:background="@drawable/bg_rectangle_black73_cr5"
            android:layout_width="0px"
            android:layout_height="match_parent"
            android:layout_weight="3.3">
            <TextView
                android:textSize="15sp"
                android:textColor="@color/white"
                android:gravity="center"
                android:layout_gravity="center"
                android:id="@+id/br_diner_price_sum"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="@string/dinner_title_price_sum"
                android:fontFamily="@font/muller_bold"/>
        </FrameLayout>
        <RelativeLayout
            android:id="@+id/br_diner_purchace_button"
            android:background="@drawable/bg_rectangle_orange_hgr_cr5"
            android:clickable="true"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1.7"
            android:alpha="0.5"
            android:layout_marginEnd="2.5dp">
            <TextView
                android:textSize="15sp"
                android:textColor="@color/white"
                android:gravity="center"
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/common_buy"
                android:drawablePadding="10dp"
                android:layout_centerInParent="true"
                android:fontFamily="@font/muller_bold"
                app:drawableLeftCompat="@drawable/ic_purchase_icon"/>
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>

<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/fragment_taxi_0"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <FrameLayout
        android:layout_gravity="center"
        android:tag="binding_1"
        android:background="@drawable/img_table"
        android:layout_width="560dp"
        android:layout_height="320dp">
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="20dp">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="5dp">
                    <TextView
                        android:textColor="@color/orange"
                        android:gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/taxi_title"
                        android:layout_centerInParent="true"
                        android:fontFamily="@font/muller_bold"
                        android:layout_alignParentStart="true"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:textSize="12sp"
                        android:textColor="@color/white"
                        android:id="@+id/br_taxi_close"
                        android:tag="binding_2"
                        android:background="@drawable/bg_rectangle_orange_hgr_cr20"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:text="@string/common_close"
                        android:layout_centerInParent="true"
                        android:textAllCaps="false"
                        android:layout_alignParentEnd="true"/>
                </RelativeLayout>
                <View
                    android:background="@drawable/bg_rectangle_gray30_white_hgr"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="20dp"/>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginTop="10dp"
                    android:paddingStart="20dp">
                    <FrameLayout
                        android:background="@drawable/bg_rectangle_orange_hgr_cr4_1"
                        android:layout_width="40dp"
                        android:layout_height="40dp">
                        <TextView
                            android:textColor="@color/white"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:id="@+id/br_taxi_rating"
                            android:tag="binding_3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/muller_bold"/>
                    </FrameLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:weightSum="2"
                        android:paddingStart="20dp">
                        <TextView
                            android:textSize="10sp"
                            android:textColor="@color/white"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:id="@+id/br_taxi_count_orders"
                            android:tag="binding_4"
                            android:layout_width="match_parent"
                            android:layout_height="0px"
                            android:layout_weight="1"
                            android:fontFamily="@font/muller_bold"/>
                        <TextView
                            android:textSize="10sp"
                            android:textColor="@color/white"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:id="@+id/br_taxi_cash"
                            android:tag="binding_5"
                            android:layout_width="match_parent"
                            android:layout_height="0px"
                            android:layout_weight="1"
                            android:fontFamily="@font/muller_bold"/>
                    </LinearLayout>
                </LinearLayout>
                <View
                    android:id="@+id/view5"
                    android:background="@drawable/bg_rectangle_gray30_white_hgr"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="20dp"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="5dp">
                    <LinearLayout
                        android:orientation="vertical"
                        android:background="@drawable/bg_rectangle_black53_cr4"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp">
                        <TextView
                            android:textSize="12sp"
                            android:textColor="@color/white"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:text="@string/taxi_info_title"
                            android:fontFamily="@font/muller_bold"/>
                        <RelativeLayout
                            android:paddingLeft="30dp"
                            android:paddingRight="30dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp">
                            <TextView
                                android:textSize="12sp"
                                android:textColor="@color/white"
                                android:id="@+id/br_taxi_type_text"
                                android:tag="binding_6"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/br_taxi_type"
                                android:fontFamily="@font/muller_bold"
                                android:layout_alignStart="@+id/br_taxi_type"
                                android:layout_alignEnd="@+id/br_taxi_type"/>
                            <TextView
                                android:textSize="10sp"
                                android:textColor="@color/orange"
                                android:id="@+id/br_taxi_type"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/taxi_order_type_title"
                                android:layout_below="@+id/br_taxi_price"
                                android:fontFamily="@font/muller_bold"
                                android:layout_alignStart="@+id/br_taxi_price"
                                android:layout_alignEnd="@+id/br_taxi_price"
                                android:layout_alignParentEnd="false"/>
                            <TextView
                                android:textSize="10sp"
                                android:textColor="@color/orange"
                                android:id="@+id/br_taxi_start"
                                android:tag="binding_7"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="20dp"
                                android:fontFamily="@font/muller_bold"
                                android:layout_alignParentStart="true"/>
                            <TextView
                                android:textSize="10sp"
                                android:textColor="@color/orange"
                                android:textColorHighlight="@color/orange"
                                android:id="@+id/br_taxi_end"
                                android:tag="binding_8"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:layout_below="@+id/br_taxi_start"
                                android:fontFamily="@font/muller_bold"
                                android:layout_alignParentStart="true"/>
                            <TextView
                                android:textSize="10sp"
                                android:textColor="@color/orange"
                                android:id="@+id/br_taxi_order_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/taxi_price"
                                android:fontFamily="@font/muller_bold"
                                android:layout_marginStart="25dp"
                                android:layout_toEndOf="@+id/br_taxi_vborder"
                                android:layout_alignParentEnd="true"/>
                            <TextView
                                android:textSize="12sp"
                                android:textColor="@color/white"
                                android:id="@+id/br_taxi_price"
                                android:tag="binding_9"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="5dp"
                                android:layout_below="@+id/br_taxi_order_text"
                                android:fontFamily="@font/muller_bold"
                                android:layout_alignStart="@+id/br_taxi_order_text"
                                android:layout_alignParentEnd="true"/>
                            <View
                                android:id="@+id/br_taxi_vborder"
                                android:background="@drawable/bg_rectangle_gray30_white_vgr"
                                android:layout_width="1dp"
                                android:layout_height="wrap_content"
                                android:layout_alignBottom="@+id/br_taxi_type_text"
                                android:layout_alignParentTop="true"
                                android:layout_centerHorizontal="true"/>
                        </RelativeLayout>
                        <TextView
                            android:textSize="10sp"
                            android:textColor="@color/orange"
                            android:id="@+id/br_taxi_dist"
                            android:tag="binding_10"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:fontFamily="@font/muller_bold"
                            android:layout_marginStart="30dp"/>
                        <TextView
                            android:textSize="10sp"
                            android:textColor="@color/orange"
                            android:id="@+id/br_taxi_time"
                            android:tag="binding_11"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="20dp"
                            android:fontFamily="@font/muller_bold"
                            android:layout_marginStart="30dp"/>
                    </LinearLayout>
                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/br_taxi_next"
                        android:tag="binding_12"
                        android:background="@drawable/ic_arrow_in_rectangle"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true"
                        android:layout_alignParentEnd="true"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/br_taxi_prev"
                        android:tag="binding_13"
                        android:background="@drawable/ic_arrow_in_rectangle"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true"
                        android:scaleX="-1"
                        android:layout_alignParentStart="true"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:paddingStart="20dp">
                    <androidx.appcompat.widget.AppCompatButton
                        android:textColor="@color/white"
                        android:id="@+id/br_taxi_accept"
                        android:tag="binding_14"
                        android:background="@drawable/bg_rectangle_orange_hgr_cr20"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:text="@string/taxi_apply_order"
                        android:layout_centerInParent="true"
                        android:textAllCaps="false"
                        android:fontFamily="@font/muller_bold"/>
                    <ImageView
                        android:id="@+id/br_taxi_comm"
                        android:tag="binding_15"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:scaleX="0.5"
                        android:scaleY="0.5"
                        android:layout_alignParentStart="true"/>
                </RelativeLayout>
            </LinearLayout>
        </ScrollView>
        <include
            android:id="@+id/blockTaxiComment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            layout="@layout/br_taxi_comment"/>
    </FrameLayout>
</FrameLayout>

<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/_8sdp"/>
            <stroke
                android:width="1dp"
                android:color="@color/light_gray_blue"/>
            <solid android:color="@color/white_15"/>
        </shape>
    </item>
    <item
        android:left="3dp"
        android:top="3dp"
        android:right="3dp"
        android:bottom="3dp">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/_8sdp"/>
            <solid android:color="@color/chlorine_green"/>
        </shape>
    </item>
</layer-list>
